<presentation>
	<viewbox x="0" y="0" width="100" height="100"/>
	<styles>
		<style name="h1"      font-opt="s20 bold underline" font-name="Tahoma" options=""/>
		<style name="h2"      font-opt="s16 bold underline" font-name="Tahoma" options=""/>
		<style name="h3"      font-opt="s12 bold underline" font-name="Tahoma" options=""/>
		<style name="cite"    font-opt="s12 italic"         font-name="Tahoma" options=""/>
		<style name="default" font-opt="s12"                font-name="Tahoma" options=""/>
	</styles>
	<part name="introduction" steps="true">
		<text step="1" style="h1"      x="00" y="00.0" w="100" h="05" content="introduction"/>
		<text step="2" style="h2"      x="00" y="07.5" w="100" h="05" content="introduction/what"/>
		<text step="3" style="cite"    x="05" y="12.5" w="095" h="20" content="introduction/cite"/>
		<text step="4" style="h2"      x="00" y="20.0" w="100" h="05" content="introduction/advantages"/>
		<text step="5" style="h3"      x="00" y="27.5" w="030" h="20" content="introduction/simplicity"/>
		<text step="5" style="default" x="30" y="27.5" w="070" h="20" content="introduction/simplicity/text"/>
		<text step="6" style="h3"      x="00" y="40.0" w="030" h="20" content="introduction/builtins"/>
		<text step="6" style="default" x="30" y="40.0" w="070" h="20" content="introduction/builtins/text"/>
		<text step="7" style="h3"      x="00" y="52.5" w="030" h="20" content="introduction/hotkeys"/>
		<text step="7" style="default" x="30" y="52.5" w="070" h="20" content="introduction/hotkeys/text"/>
		<text step="8" style="h3"      x="00" y="65.0" w="030" h="20" content="introduction/automation"/>
		<text step="8" style="default" x="30" y="65.0" w="070" h="20" content="introduction/automation/text"/>
		<text step="9" style="h3"      x="00" y="80.0" w="030" h="20" content="introduction/advanced"/>
		<text step="9" style="default" x="30" y="80.0" w="070" h="20" content="introduction/advanced/text"/>
	</part>
	<part name="version-history" steps="true">
		<text    step="1" style="h1" x="00" y="0.0" w="100" h="5" content="version-history"/>
		<!--picture step="2" x="00" y="7.5">C:\Dokumente und Einstellungen\___\Desktop\gFS\resources\diagram.png</picture-->
		<browser step="2" x="00" y="7.5" w="100" h="90" resource="versions.svg"/>
	</part>
	<part name="usage-cases"/>
	<part name="script-structure" steps="true">
		<text   step="1" style="h1"      x="00" y="00.0" w="100" h="05" content="script-structure"/>
		<text   step="2" style="h2"      x="00" y="07.5" w="100" h="05" content="script-structure/file"/>
		<text   step="3" style="default" x="05" y="12.5" w="075" h="10" content="script-structure/file/text"/>
		<button step="3" style="default" x="05" y="20.0" w="020" h="05">
			Notepad++
			<event name="click" handler-type="function" handler="RunEditor"/>
		</button>
		<text   step="4" style="h2"      x="00" y="30.0" w="100" h="05" content="script-structure/auto-execute"/>
		<text   step="5" style="default" x="05" y="35.0" w="075" h="20" content="script-structure/auto-execute/text"/>

		<part name="functions" steps="true">
			<text step="1" style="h2"      x="00" y="00.0" w="100" h="05" content="script-structure/functions"/>
			<text step="2" style="default" x="05" y="05.0" w="095" h="20" content="script-structure/functions/text"/>
			<edit step="3"                 x="05" y="12.5" w="095" h="25">
Hello()
{
	; comment
	MsgBox Hello
}
			</edit>
		</part>
	</part>
	<part name="functionality">
		<part name="gui"/>
		<part name="hotkeys"/>
		<part name="automation"/>
		<part name="extensibility"/>
	</part>
	<part name="community"/>
	<part name="end"/>
</presentation>